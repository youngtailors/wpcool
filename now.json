{
  "name": "wpcool",
  "alias": "wpcool",
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@now/node@canary"
    },
    {
      "src": "website/next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "src/index.ts"
    },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
